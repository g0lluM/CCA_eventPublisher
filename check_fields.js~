function check()
{
    var $place = document.getElementById("eventPlace").value;
    var $date = document.getElementById("eventDate").value;
    
    if ($place == null || $place == "")
    {
	alert("You have to specify a date and a place. The abstract is not compulsory");
	return false;
    }
    if ($date == null || $date == "")
    {
	alert("You have to specify a date and a place. The abstract is not compulsory");
	return false;
    }
}
